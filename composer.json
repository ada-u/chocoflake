{
  "name": "ada-u/chocoflake",
  "type": "library",
  "description": "64bit time based id generator",
  "keywords": [ "chocoflake", "id", "generator", "idgen" ],
  "homepage": "https://github.com/ada-u/chocoflake",
  "authors": [
    { "name": "ada-u", "email": "ada-u@ada-u.com" }
  ],
  "license": "MIT",
  "require-dev": {
    "fabpot/php-cs-fixer": "dev-master",
    "phpunit/phpunit": "^9"
  },
  "require": {
    "php": ">=8.0",
    "mockery/mockery": "1.4.* || 1.5.* || 1.6.*",
    "predis/predis": "1.1.* || 2.0.* || 2.1.* || 2.2.*",
    "ext-sysvsem": "*",
    "ext-sysvshm": "*"
  },
  "autoload": {
    "psr-4": {
      "Adachi\\Choco\\": "src/Choco/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Adachi\\Tests\\": "tests/Tests/"
    }
  },
  "config": {
    "sort-packages": true
  }
}
